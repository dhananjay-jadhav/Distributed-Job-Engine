{
  "name": "auth-db",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/auth-db/src",
  "projectType": "library",
  "tags": [],
  "// targets": "to see all targets run: nx show project auth-db --web",
  "targets": {
    "build": {
      "dependsOn": ["gen-types"]
    },
    "gen-types" : {
      "command": "prisma generate",
      "options": {
        "cwd": "libs/auth-db/src/lib"
      }
    },
    "migrate-prisma" : {
      "command": "prisma migrate dev",
      "options": {
        "cwd": "libs/auth-db/src/lib"
      }
    }
  }
}
